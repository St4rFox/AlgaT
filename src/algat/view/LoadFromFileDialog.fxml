<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="algat.controller.LoadFromFileDialogController">
    <children>
        <VBox layoutX="220.0" layoutY="122.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox prefWidth="200.0">
                    <children>
                        <TextField fx:id="selectedFilePath" editable="false" style="-fx-background-radius: 0 0 0 0; -fx-background-color: transparent; -fx-border-width: 1px 0 1px 1px; -fx-border-style: solid;" HBox.hgrow="ALWAYS" />
                        <Button fx:id="folderButton" contentDisplay="CENTER" mnemonicParsing="false" onAction="#folderButtonPressed" style="-fx-background-radius: 0 0 0 0; -fx-background-color: transparent; -fx-border-style: solid; -fx-border-width: 1px 1px 1px 0;">
                            <graphic>
                                <ImageView fitHeight="27.0" fitWidth="17.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../resources/images/folder.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                    </children>
                    <padding>
                        <Insets left="30.0" right="30.0" />
                    </padding>
                    <VBox.margin>
                        <Insets bottom="20.0" />
                    </VBox.margin>
                </HBox>
                <TableView fx:id="tableView" editable="true" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                        <TableColumn fx:id="keyColumn" prefWidth="200.0" sortable="false" text="Key" />
                        <TableColumn fx:id="valueColumn" prefWidth="75.0" sortable="false" text="Value" />
                    </columns>
                    <VBox.margin>
                        <Insets bottom="20.0" left="30.0" right="30.0" />
                    </VBox.margin>
